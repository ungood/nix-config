{
  "permissions": {
    "allow": [
      "Bash(git:*)",
      "Bash(gh:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(ls:*)",
      "Bash(find:*)",
      "WebFetch",
      "WebSearch"
    ]
  },
  "extraKnownMarketplaces": {
    "claude-code-plugins": {
      "source": "github",
      "repo": "anthropics/claude-code"
    }
  },
  "enabledPlugins": {
    "commit-commands@claude-code-plugins": true,
    "feature-dev@claude-code-plugins": true
  }
}
